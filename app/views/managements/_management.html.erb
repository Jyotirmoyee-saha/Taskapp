<html lang="en">
<head>
  <title>Bootstrap Example</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>

<div class="container">
  <h2>Basic Progress Bar</h2>
  <div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:30%">
      <span class="sr-only">70% Complete</span>
    </div>
  </div>
</div>

</body>
</html>


<div id="<%= dom_id management %>">

  <p>
    <strong>User:</strong>
    <%= management.user_id %>
  </p>
 

  <p>
    <strong>Project:</strong>
    <%= management.project_id %>
  </p>

  <p>
    <strong>Project Name:</strong>
    <%= management.project.title %>
  </p>

   <% if management.tasks.any? %>
    <div class="task-list">
      <h4>Tasks:</h4>
      <ul>
        <%management.tasks.each do |task| %>
          <li><%= task.tname %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
   
</div>
